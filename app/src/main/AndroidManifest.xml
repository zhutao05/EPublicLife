<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.cimcitech.epubliclife">

    <application
        android:name=".ApkApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".activity.login.LoginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".activity.login.ForgetPwdActivity" />
        <activity android:name=".activity.login.ProtocolActivity" />
        <activity android:name=".activity.login.RegisterActivity" />
        <activity android:name=".activity.login.WelcomeActivity" />
        <activity android:name=".MainActivity" />
    </application>

</manifest>